{
    "type": "origins:multiple",
    "name": "Gift of the Skies",
    "description": "Evolution gifted you wings, which you can use to fly around with ease.",
    "badges": [
        {
            "type": "origins:keybind",
            "sprite": "origins:textures/gui/badge/active.png",
            "text": "Use %s to launch high into the air when grounded, or to ascend while gliding."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/arrow_up.png",
            "text": "You have limited Stamina. Launching and ascending use stamina; regain by gliding or resting on the ground."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": "Trying to launch or ascend with heavy armor (>12 Armor) is impossible."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": "It's too dangerous to use fireworks while gliding."
        }
    ],
    "vanilla_wings": {
        "type": "origins:elytra_flight",
        "render_elytra": true,
        "condition": {
            "type": "origins:origin",
            "layer": "originstweaks:elytrian_options",
            "origin": "originstweaks:vanilla_elytra"
        }
    },
    "custom_wings": {
        "type": "origins:elytra_flight",
        "render_elytra": true,
        "texture_location": "originstweaks:textures/entity/wings.png",
        "condition": {
            "type": "origins:origin",
            "layer": "originstweaks:elytrian_options",
            "origin": "originstweaks:custom_elytra"
        }
    },
    "launch": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:if_else_list",
            "actions": [
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:command",
                                "command": "attribute @s minecraft:generic.armor get",
                                "comparison": "<=",
                                "compare_to": 12
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_stamina",
                                "comparison": ">=",
                                "compare_to": 120
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:add_velocity",
                                "y": 2,
                                "client": false,
                                "operation": "set"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_stamina",
                                "change": -120,
                                "operation": "add"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "playsound minecraft:entity.parrot.fly player @a ~ ~ ~ 1 0.5"
                            }
                        ]
                    }
                },
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:command",
                                "command": "attribute @s minecraft:generic.armor get",
                                "comparison": "<=",
                                "compare_to": 12
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_stamina",
                                "comparison": "<",
                                "compare_to": 120
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "title @s actionbar \"You are too exhausted to launch.\""
                    }
                },
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:command",
                                "command": "attribute @s minecraft:generic.armor get",
                                "comparison": ">",
                                "compare_to": 12
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "title @s actionbar \"Your armor is too heavy for launching.\""
                    }
                }
            ]
        },
        "cooldown": 200,
        "hud_render": {
            "bar_index": 3
        },
        "key": {
            "key": "key.origins.primary_active",
            "continuous": true
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:on_block"
                },
                {
                    "type": "origins:fall_flying",
                    "inverted": true
                }
            ]
        }
    },
    "ascend": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:if_else_list",
            "actions": [
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:command",
                                "command": "attribute @s minecraft:generic.armor get",
                                "comparison": "<=",
                                "compare_to": 12
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_stamina",
                                "comparison": "!=",
                                "compare_to": 0
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:add_velocity",
                                "y": 0.75,
                                "z": 0.25,
                                "client": false,
                                "space": "local_horizontal_normalized",
                                "operation": "set"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "playsound minecraft:entity.phantom.flap player @a ~ ~ ~ 1 2"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_stamina",
                                "change": -30,
                                "operation": "add"
                            }
                        ]
                    }
                },
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:command",
                                "command": "attribute @s minecraft:generic.armor get",
                                "comparison": "<=",
                                "compare_to": 12
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_stamina",
                                "comparison": "==",
                                "compare_to": 0
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "title @s actionbar \"You are too exhausted to ascend.\""
                    }
                },
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:command",
                                "command": "attribute @s minecraft:generic.armor get",
                                "comparison": ">",
                                "compare_to": 12
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "title @s actionbar \"Your armor is too heavy to ascend.\""
                    }
                }
            ]
        },
        "cooldown": 20,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.origins.primary_active",
            "continuous": true
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:on_block",
                    "inverted": true
                },
                {
                    "type": "origins:fall_flying"
                }
            ]
        }
    },
    "stamina": {
        "type": "origins:resource",
        "min": 0,
        "max": 600,
        "start_value": 600,
        "hud_render": {
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 2,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_stamina",
                "compare_to": 600,
                "comparison": "!="
            }
        }
    },
    "stamina_regenerate": {
        "type": "origins:action_over_time",
        "interval": 3,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_stamina",
            "change": 6
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:on_block"
                },
                {
                    "type": "origins:fall_flying",
                    "inverted": true
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_stamina",
                    "comparison": "!=",
                    "compare_to": 600
                }
            ]
        }
    },
    "stamina_sleep": {
        "type": "origins:action_on_wake_up",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_stamina",
            "change": 600,
            "operation": "set"
        }
    },
    "stamina_reincarnation": {
        "type": "origins:action_on_callback",
        "entity_action_respawned": {
            "type": "origins:change_resource",
            "resource": "*:*_stamina",
            "change": 600,
            "operation": "set"
        }
    },
    "restrict_fireworks": {
        "type": "origins:prevent_item_use",
        "item_condition": {
            "type": "origins:ingredient",
            "ingredient": {
                "item": "minecraft:firework_rocket"
            }
        },
        "condition": {
            "type": "origins:fall_flying"
        }
    }
}