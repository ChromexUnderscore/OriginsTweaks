{
    "type": "origins:multiple",
    "hidden": true,
    "idle": {
        "type": "origins:action_over_time",
        "interval": 100,
        "entity_action": {
            "type": "origins:chance",
            "action": {
                "type": "origins:if_else_list",
                "actions": [
                    {
                        "condition": {
                            "type": "origins:health",
                            "comparison": ">",
                            "compare_to": 4
                        },
                        "action": {
                            "type": "origins:play_sound",
                            "sound": "minecraft:entity.shulker.ambient"
                        }
                    },
                    {
                        "condition": {
                            "type": "origins:health",
                            "comparison": "<=",
                            "compare_to": 4
                        },
                        "action": {
                            "type": "origins:play_sound",
                            "sound": "minecraft:entity.shulker.ambient",
                            "pitch": 0.8
                        }
                    }
                ]
            },
            "chance": 0.05
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:sneaking",
                    "inverted": true
                },
                {
                    "type": "origins:gamemode",
                    "gamemode": "spectator",
                    "inverted": true
                },
                {
                    "type": "origins:status_effect",
                    "effect": "minecraft:invisibility",
                    "inverted": true
                }
            ]
        }
    },
    "hurt_death": {
        "type": "origins:self_action_when_hit",
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:health",
                "comparison": ">",
                "compare_to": 0
            },
            "if_action": {
                "type": "origins:play_sound",
                "sound": "minecraft:entity.shulker.hurt"
            },
            "else_action": {
                "type": "origins:play_sound",
                "sound": "minecraft:entity.shulker.death"
            }

        },
        "cooldown": 1
    }
}